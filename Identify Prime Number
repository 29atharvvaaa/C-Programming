#include <stdio.h>
int main() {
    int i,nos;
    printf("Enter a Number.\n");
    scanf("%d", &nos);
    if (nos<=1)
    printf("The given number is NOT a Prime Number.\n");
    else {
        for (i=2; i<=nos; i++){
            if (nos % i == 0 )
            break;
        }
        if (i == nos)
        printf("The given number is a Prime Number.\n");
        else
        printf("The given number is NOT a Prime Number.\n");
    }
    return 0;
}
