#include <stdio.h>
int main() {
    int op, n1, n2, ans;
    printf("Enter an Operation\n1:+\n2:-\n3:*\n4:/\n");
    scanf("%d", &op);
    printf("Enter 1st Number:\n");
    scanf("%d", &n1);
    printf("Enter 2nd Number:\n");
    scanf("%d", &n2);
    switch(op) {
        case 1:
        ans=n1+n2;
        printf("Result:\n%d\n", ans);
        break;
        case 2:
        ans=n1-n2;
        printf("Result:\n%d\n", ans);
        break;
        case 3:
        ans=n1*n2;
        printf("Result:\n%d\n", ans);
        break;
        case 4:
        ans=n1/n2;
        printf("Result:\n%d\n", ans);
        break;
    }
    return 0;
}
